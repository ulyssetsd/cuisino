{
    "name": "cuisino",
    "version": "1.0.0",
    "description": "Automatic HelloFresh recipe extractor from front/back photos",
    "type": "module",
    "main": "dist/main.js",
    "scripts": {
        "build": "tsup",
        "build:watch": "tsup --watch",
        "dev": "tsx src/main.ts",
        "start": "node dist/main.js",
        "extraction:run": "tsx src/scripts/extract-only.ts",
        "quality:validate": "tsx src/scripts/validate-quality.ts",
        "analysis:report": "tsx src/scripts/generate-report.ts",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "test:run": "vitest run",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write \"src/**/*.{ts,json,md}\"",
        "format:check": "prettier --check \"src/**/*.{ts,json,md}\"",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "recipes",
        "hellofresh",
        "extraction",
        "ocr",
        "ai"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "dotenv": "^16.5.0",
        "fs-extra": "^11.3.0",
        "openai": "^4.103.0",
        "sharp": "^0.34.2"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.14.0",
        "@typescript-eslint/eslint-plugin": "^7.13.0",
        "@typescript-eslint/parser": "^7.13.0",
        "eslint": "^8.57.0",
        "prettier": "^3.5.3",
        "ts-node": "^10.9.2",
        "tsup": "^8.1.0",
        "tsx": "^4.20.2",
        "typescript": "^5.4.5",
        "vitest": "^1.6.0"
    }
}
